#: ../ui_func.py:48
msgid "Threshold for pulling up the fishing rod"
msgstr "釣竿を引き揚げるしきい値"

#: ../ui_func.py:50 ../ui_func.py:66
msgid "Option"
msgstr "オプション"

#: ../ui_func.py:51
msgid "Audio input device"
msgstr "録音デバイス"

#: ../ui_func.py:52
msgid "Automatically minimize windows when launching tools"
msgstr "ツール開始時に自動的にウィンドウを最小化"

#: ../ui_func.py:53
